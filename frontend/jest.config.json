{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testMatch": [
    "**/tests/e2e/**/*.spec.ts"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "testTimeout": 120000,
  "setupFilesAfterEnv": [
    "<rootDir>/tests/setup.ts"
  ]
}