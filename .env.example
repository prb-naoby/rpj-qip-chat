# Copy this file to .env and fill in real values

# ======================
# Required API Keys
# ======================
OPENAI_API_KEY=sk-your-openai-key
GOOGLE_API_KEY=your-google-api-key

# LLM Model (optional - defaults to gpt-4o-mini)
PANDASAI_LLM_MODEL=gpt-4o-mini

# ======================
# Upload Settings
# ======================
UPLOAD_MAX_MB=25

# ======================
# Microsoft OneDrive (Optional)
# ======================
# Leave blank to disable OneDrive integration
MS_TENANT_ID=
MS_CLIENT_ID=
MS_CLIENT_SECRET=
ONEDRIVE_DRIVE_ID=
ONEDRIVE_ROOT_PATH=

# ======================
# Debug
# ======================
DEBUG_MODE=false
